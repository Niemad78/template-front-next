{
  "extends": "next/core-web-vitals",
  "plugins": [
    "react",
    "prettier",
    "unused-imports"
  ],
  "rules": {
    "no-console": "error",
    "no-alert": "error",
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "camelcase": "error",
    "react/jsx-indent": [2, 2],
    "react/jsx-curly-newline": 1,
    "react/jsx-props-no-spreading": 1,
    "prettier/prettier": "error",
    "unused-imports/no-unused-imports": "error",
    "no-var": "error"
  }
}
